    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.1.4",
    "@types/cookie-parser": "^1.4.3",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.1.0",
